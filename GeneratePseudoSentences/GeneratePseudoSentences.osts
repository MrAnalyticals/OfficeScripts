{"version":"0.2.0","body":"function main(workbook: ExcelScript.Workbook) {\n    const vowels = [\"a\", \"e\", \"i\", \"o\", \"u\"];\nlet PseudoTable = workbook.getTable('Pseudo') \n    const generateString = (length: number) => {\n        let result = \"\";\n        for (let i = 0; i < length; i++) {\n            if (length === 1) {\n                result += (Math.random() < 0.5) ? \"a\" : \"i\";\n            } else {\n                result += String.fromCharCode(97 + Math.floor(Math.random() * 26));\n            }\n        }\n        return result;\n    }\n\n    const generateSentence = () => {\n        let sentence = \"\";\n        for (let i = 0; i < 15; i++) {\n            let length = Math.floor(Math.random() * 10) + 1;\n            if (length === 3) {\n                sentence += generateString(length) + vowels[Math.floor(Math.random() * 5)] + \" \";\n            } else if (length === 4) {\n                sentence += generateString(length) + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + \" \";\n            } else if (length >= 5 && length <= 7) {\n                sentence += generateString(length) + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + \" \";\n            } else {\n                sentence += generateString(length) + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + vowels[Math.floor(Math.random() * 5)] + \" \";\n            }\n        }\n        return sentence;\n    }\n    let SentenceResult:string = generateSentence().toString()\n    PseudoTable.addRow(-1, [SentenceResult]);\n  // Set wrap text to true for table table1 body\n  PseudoTable.getRangeBetweenHeaderAndTotal().getFormat().setWrapText(true);\n}","description":"","parameterInfo":"{\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}