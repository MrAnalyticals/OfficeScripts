function Linesofthree(workbook: ExcelScript.Workbook): boolean {
  let selectedSheet = workbook.getWorksheet('TICTAC');
  //Get the cells in the range
  let e7 = selectedSheet.getRange('e7').getValue()
  let e8 = selectedSheet.getRange('e8').getValue()
  let e9 = selectedSheet.getRange('e9').getValue()
  let f7 = selectedSheet.getRange('f7').getValue()
  let f8 = selectedSheet.getRange('f8').getValue()
  let f9 = selectedSheet.getRange('f9').getValue()
  let g7 = selectedSheet.getRange('g7').getValue()
  let g8 = selectedSheet.getRange('g8').getValue()
  let g9 = selectedSheet.getRange('g9').getValue()

  if (((e7 === e8) && (e7 === e9)) && e7 != '') {
    return true
  }
  else if (((e7 === f8) && (e7 === g9)) && e7 != '') {
    return true
  }
  else if (((f7 === f8) && (f7 === f9)) && f7 != '') {
    return true
  }
  else if (((g7 === g8) && (g7 === g9)) && g7 != '') {
    return true
  }
  else if (((e7 == f7) && (e7 == g7)) && e7 != '') {
    return true
  }
  else if (((e8 === f8) && (e8 === g8)) && e8 != '') {
    return true
  }
  else if (((e9 === f9) && (e9 === g9)) && e9 != '') {
    return true
  }
  else if (((e9 === f8) && (e9 === g7)) && e9 != '') {
    return true
  }
  else {
    return false
  }
}

function GetRangeCount(workbook: ExcelScript.Workbook): number {
  let selectedSheet = workbook.getWorksheet('TICTAC');
  let RangeStr = selectedSheet.getRange('e7:g9')
  //x,X,,x,o,,O,x,
  let Rangecount: number
  Rangecount = 0
  let RangeStrValues = RangeStr.getValues()
  // Count number of markers in the Input Range
  RangeStrValues.forEach((rowItem, rowIndex) => {
    RangeStrValues[rowIndex].forEach((columnItem, columnIndex) => {
      let cellValues = RangeStr.getCell(rowIndex, columnIndex).getValue()
      if (cellValues != '') {
        Rangecount++
      }
    })
  })
  return Rangecount
}

function EnterMarker(workbook: ExcelScript.Workbook) {
  let selectedSheet = workbook.getWorksheet('TICTAC');
  //find an empty space. Build an array of the empty cell addresses?
  let e7 = selectedSheet.getRange('e7').getValue()
  let e8 = selectedSheet.getRange('e8').getValue()
  let e9 = selectedSheet.getRange('e9').getValue()
  let f7 = selectedSheet.getRange('f7').getValue()
