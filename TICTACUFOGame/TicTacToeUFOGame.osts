function gridLocationCol1(rowN: number, colN: number): string {
  let rowNN: number
  let colNN: string = 's'
  if (rowN == 0) {
    rowNN = 2
  }
  if (rowN == 1) {
    rowNN = 3
  }
  if (rowN == 3) {
    rowNN = 4
  }
  if (rowN == 4) {
    rowNN = 5
  }
  if (rowN == 5) {
    rowNN = 6
  }
  if (rowN == 6) {
    rowNN = 7
  }
  if (rowN == 7) {
    rowNN = 8
  }
  if (rowN == 8) {
    rowNN = 9
  }
  if (rowN == 9) {
    rowNN = 10
  }

  let returnVal = colNN + rowNN  //= `$(colNN,rowNN)` 
  //console.log(returnVal)
  return returnVal
}

function gridLocation(rowN: number, colN: number): string {
  let rowNN: number
  let colNN: string
  if (rowN == 0) {
    rowNN = 7
  }
  if (rowN == 1) {
    rowNN = 8
  }
  if (rowN == 2) {
    rowNN = 9
  }
  if (rowN > 2) {
    return 'Err'
  }
  if (colN == 0) {
    colNN = 'e'
  }
  if (colN == 1) {
    colNN = 'f'
  }
  if (colN == 2) {
    colNN = 'g'
  }
  if (colN > 2) {
    return 'Err'
  }
  let returnVal = colNN + rowNN.toString()  //= `$(colNN,rowNN)` 
  //console.log(returnVal)
  return returnVal
}

