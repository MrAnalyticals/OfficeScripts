function main(workbook: ExcelScript.Workbook) {
  let selectedSheet = workbook.getWorksheet('TICTAC')
  let SheetAlien = workbook.getWorksheet('ALIEN')
  let M6 = selectedSheet.getRange('M6').getValue()
  let P8 = selectedSheet.getRange('P8').getValue()
  let RangeCountInt: number
  let Table1 = selectedSheet.getTable('Table1')

  if (P8 == 1 && M6 == 1) {
    selectedSheet.getRange('L4').setValue('Error. You have a 1 in both cells P8 and M6! Remove one before proceeding.')
    return
  }
  if (P8 == 1 && M6 != 1) {
    selectedSheet.getRange('L4').setValue('Enter a 1 in cell M6 or a O marker in the grid to start playing. ')
    selectedSheet.getRange('M6').setValue('')
    selectedSheet.getRange('P8').setValue('')
    selectedSheet.getRange("E7:G9")
      .clear(ExcelScript.ClearApplyTo.contents)
    // Delete 8 row(s) at index 1 from table Table1
    let rowCount: number = Table1.getRowCount()
    if (rowCount > 1) {
      console.log(rowCount)
      Table1.deleteRowsAt(1, rowCount - 1)
    }
    // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
    selectedSheet.getRange("S2")
      .clear(ExcelScript.ClearApplyTo.contents)
    return
  }
  if (M6 == 1) {
    selectedSheet.getRange('L4').setValue('')
    selectedSheet.getRange('M6').setValue('')
    selectedSheet.getRange('P8').setValue('')
    selectedSheet.getRange("E7:G9")
      .clear(ExcelScript.ClearApplyTo.contents)
    // Delete 8 row(s) at index 1 from table Table1
    let rowCount: number = Table1.getRowCount()
    if (rowCount > 1) {
      console.log(rowCount)
      Table1.deleteRowsAt(1, rowCount - 1)
    }
    // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
    selectedSheet.getRange("S2")
      .clear(ExcelScript.ClearApplyTo.contents)
    EnterMarker(workbook)
    selectedSheet.getRange('M2').setValue(1)
    return
  }
  //Test value of setValue(above) 
  // if (Count of values in range = M2) then (a new marker was added)
  let Rangecount: number
  Rangecount = GetRangeCount(workbook)
  let TurnCountStr: string = selectedSheet.getRange('M2').getValue().toString()
  let TurnCount: number
  if (TurnCountStr == '') {
    TurnCount = 0
  } else {
    TurnCount = parseInt(TurnCountStr)
  }
  console.log('Rangecount: ' + Rangecount)
  console.log('TurnCount: ' + TurnCount)
  if (Rangecount < 9) {
    //Clear Message box
    selectedSheet.getRange('L4').setValue('')
    // Test if there are any lines of 3 in a row
    if (Linesofthree(workbook) == true) {
      if (LineofThreeXs(workbook) == true) {
        selectedSheet.getRange('L4').setValue('Game Over! Excel won!')
        flashColours(workbook)
        //write function ClearSheetVals(workbook)
        selectedSheet.getRange('M2').setValue('')
        selectedSheet.getRange('M6').setValue('')
        selectedSheet.getRange("E7:G9").clear(ExcelScript.ClearApplyTo.contents)
        //SheetAlien.activate()
        //DisplayGreenMan(workbook)
        // Delete 8 row(s) at index 1 from table Table1
        let rowCount: number = Table1.getRowCount()
        if (rowCount > 1) {
          console.log(rowCount)
          Table1.deleteRowsAt(1, rowCount - 1)
        }
        // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
        selectedSheet.getRange("S2")
          .clear(ExcelScript.ClearApplyTo.contents)
        return
      } else {
        selectedSheet.getRange('L4').setValue('Game Over! You won!')
        flashColours(workbook)
        SheetAlien.setVisibility(ExcelScript.SheetVisibility.visible)
        SheetAlien.activate()
        selectedSheet.getRange('M2').setValue('')
        selectedSheet.getRange('M6').setValue('')
        selectedSheet.getRange("E7:G9")
          .clear(ExcelScript.ClearApplyTo.contents)
        // Delete 8 row(s) at index 1 from table Table1
        let rowCount: number = Table1.getRowCount()
        if (rowCount > 1) {
          console.log(rowCount)
          Table1.deleteRowsAt(1, rowCount - 1)
        }
        // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
        selectedSheet.getRange("S2")
          .clear(ExcelScript.ClearApplyTo.contents)
        DisplayGreenMan(workbook)
        SheetAlien.setVisibility(ExcelScript.SheetVisibility.hidden)
        console.log('Routine Finished')
        return
      }
    } else {
      if (Rangecount > TurnCount) {
        //I am assuming human entered a marker here. Test marker value. 
        let gridRange = selectedSheet.getRange("E7:G9")
        //TO DO : Create a test for 'o' (or !=x) being added first. If it is then add its locaton in Table1. If is x or X Return with error message removing that marker.
        //Iterate through the grid finding a cell that location that is not listed in Table1. This should, then, be added to Table1.
        let loca = findLocation(workbook)
        AddMarkerLocationToTable1(workbook, loca)
        //
        EnterMarker(workbook)
        // Update TurnCount
        RangeCountInt = GetRangeCount(workbook)
        selectedSheet.getRange('M2').setValue(RangeCountInt)
        //Determine who won after the marker was applied
        if (Linesofthree(workbook) == true) {
          if (LineofThreeXs(workbook) == true) {
            selectedSheet.getRange('L4').setValue('Game Over! Excel won!')
            flashColours(workbook)
            selectedSheet.getRange('M2').setValue('')
            selectedSheet.getRange('M6').setValue('')
            selectedSheet.getRange("E7:G9")
              .clear(ExcelScript.ClearApplyTo.contents)
            //SheetAlien.activate()
            //DisplayGreenMan(workbook)
            // Delete 8 row(s) at index 1 from table Table1
            let rowCount: number = Table1.getRowCount()
            if (rowCount > 1) {
              console.log(rowCount)
              Table1.deleteRowsAt(1, rowCount - 1)
            }
            // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
            selectedSheet.getRange("S2")
              .clear(ExcelScript.ClearApplyTo.contents)
            return
          } else {
            selectedSheet.getRange('L4').setValue('Game Over! You won!')
            flashColours(workbook)
            SheetAlien.setVisibility(ExcelScript.SheetVisibility.visible)
            SheetAlien.activate()
            selectedSheet.getRange('M2').setValue('')
            selectedSheet.getRange('M6').setValue('')
            selectedSheet.getRange("E7:G9")
              .clear(ExcelScript.ClearApplyTo.contents)
            DisplayGreenMan(workbook)
            SheetAlien.setVisibility(ExcelScript.SheetVisibility.hidden)
            console.log('Routine Finished')
            // Delete 8 row(s) at index 1 from table Table1
            let rowCount: number = Table1.getRowCount()
            if (rowCount > 1) {
              console.log(rowCount)
              Table1.deleteRowsAt(1, rowCount - 1)
            }
            // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
            selectedSheet.getRange("S2")
              .clear(ExcelScript.ClearApplyTo.contents)
            return
          }
        }
      }
      else { //Rangecount !> TurnCount

      }
    }
  }   //brackets

  if (Rangecount == 9) {
    if (Linesofthree(workbook) == true) {
      if (LineofThreeXs(workbook) == true) {
        selectedSheet.getRange('L4').setValue('Game Over! Excel won!')
        flashColours(workbook)
        selectedSheet.getRange('M2').setValue('')
        selectedSheet.getRange('M6').setValue('')
        selectedSheet.getRange("E7:G9")
          .clear(ExcelScript.ClearApplyTo.contents)
        //SheetAlien.activate()
        //DisplayGreenMan(workbook)
        console.log('Routine Finished')
        // Delete 8 row(s) at index 1 from table Table1
        let rowCount: number = Table1.getRowCount()
        if (rowCount > 1) {
          console.log(rowCount)
          Table1.deleteRowsAt(1, rowCount - 1)
        }
        // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
        selectedSheet.getRange("S2")
          .clear(ExcelScript.ClearApplyTo.contents)
        return
      } else {
        selectedSheet.getRange('L4').setValue('Game Over! You won!')
        flashColours(workbook)
        SheetAlien.setVisibility(ExcelScript.SheetVisibility.visible)
        SheetAlien.activate()
        selectedSheet.getRange('M2').setValue('')
        selectedSheet.getRange('M6').setValue('')
        selectedSheet.getRange("E7:G9")
          .clear(ExcelScript.ClearApplyTo.contents)
        DisplayGreenMan(workbook)
        SheetAlien.setVisibility(ExcelScript.SheetVisibility.hidden)
        console.log('Routine Finished')
        // Delete 8 row(s) at index 1 from table Table1
        let rowCount: number = Table1.getRowCount()
        if (rowCount > 1) {
          console.log(rowCount)
          Table1.deleteRowsAt(1, rowCount - 1)
        }
        // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
        selectedSheet.getRange("S2")
          .clear(ExcelScript.ClearApplyTo.contents)
        return
      }
    } else {
      selectedSheet.getRange('L4').setValue('Game Over! No one won!')
      selectedSheet.getRange('M2').setValue('')
      selectedSheet.getRange('M6').setValue('')
      selectedSheet.getRange("E7:G9")
        .clear(ExcelScript.ClearApplyTo.contents)
      console.log('Routine Finished')
      // Delete 8 row(s) at index 1 from table Table1
      let rowCount: number = Table1.getRowCount()
      if (rowCount > 1) {
        console.log(rowCount)
        Table1.deleteRowsAt(1, rowCount - 1)
      }
      // Clear ExcelScript.ClearApplyTo.contents from range TICTAC!S2
      selectedSheet.getRange("S2")
        .clear(ExcelScript.ClearApplyTo.contents)
      return
    }
  }
  console.log('Routine finished.')
}

