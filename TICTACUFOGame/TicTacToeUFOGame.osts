function AddMarkerLocationToTable1(workbook: ExcelScript.Workbook, location: string) {
  //Add marker to the next available row in Table1
  //Find cell ref of next empty cell
  let TICTACSheet = workbook.getWorksheet('TICTAC')
  let Table1 = TICTACSheet.getTable('Table1')
  let Table1Range = Table1.getRange()
  let rowCount: number = Table1Range.getRowCount()
  let s2 = TICTACSheet.getRange('s2').getValue().toString()
  if (s2 == '') {
    TICTACSheet.getRange('s2').setValue(location)
  } else {
    rowCount++
    let orderNo: number = rowCount - 1
    let rowCountStr = rowCount.toString()
    TICTACSheet.getRange('s' + rowCountStr).setValue(location)
    TICTACSheet.getRange('r' + rowCountStr).setValue(orderNo)
  }
}

